<?xml version="1.0" encoding="UTF-8"?>
<settings xmlns="http://maven.apache.org/SETTINGS/1.0.0"
          xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
          xsi:schemaLocation="http://maven.apache.org/SETTINGS/1.0.0 http://maven.apache.org/xsd/settings-1.0.0.xsd">

    <!--  Cette section vous permet de spécifier des groupes de plugins Maven-->
    <pluginGroups></pluginGroups>
    <!--Vous pouvez spécifier des informations de proxy dans cette section-->
    <proxies></proxies>

    <!--Cette section spécifie les serveurs Maven que Maven utilisera pour diverses opérations-->
    <servers>
        <server>
            <id>maven-private-nas</id>
            <username>admin</username>
            <password>x4LLM9JSq3iwTPthoqEk</password>
        </server>

    </servers>

    <mirrors>
        <!-- Mirror pour Nexus -->
        <mirror>
            <id>maven-central</id>
            <mirrorOf>*</mirrorOf>
            <url>https://sonatype-nexus.backhole.ovh/repository/maven-central/</url>
        </mirror>
    </mirrors>

    <!--Les profils permettent de définir des configurations spécifiques pour différents environnements-->
    <profiles>
        <profile>

            <repositories>

                <!-- Référence à votre dépôt maven-Nas dans Nexus (privé) -->
                <repository>
                    <id>maven-repo-nas</id>
                    <url>https://sonatype-nexus.backhole.ovh/repository/maven-private-nas/</url>
                    <releases>
                        <enabled>true</enabled>
                    </releases>
                    <snapshots>
                        <enabled>true</enabled>
                    </snapshots>
                </repository>

                <!-- Référence à votre dépôt maven-central dans Nexus -->
                <repository>
                    <id>maven-central</id>
                    <url>https://sonatype-nexus.backhole.ovh/repository/maven-central</url>
                    <releases>
                        <enabled>true</enabled>
                    </releases>
                    <snapshots>
                        <enabled>true</enabled>
                    </snapshots>
                </repository>

                <!-- Référence à votre dépôt maven-public dans Nexus -->
                <repository>
                    <id>maven-public</id>
                    <url>https://sonatype-nexus.backhole.ovh/repository/maven-public</url>
                    <releases>
                        <enabled>true</enabled>
                    </releases>
                    <snapshots>
                        <enabled>true</enabled>
                    </snapshots>
                </repository>

                <!-- Référence à votre dépôt maven-releases dans Nexus -->
                <repository>
                    <id>maven-releases</id>
                    <url>https://sonatype-nexus.backhole.ovh/repository/maven-releases</url>
                    <releases>
                        <enabled>true</enabled>
                    </releases>
                    <snapshots>
                        <enabled>false</enabled>
                    </snapshots>
                </repository>


                <!-- Référence à votre dépôt maven-snapshots dans Nexus -->
                <repository>
                    <id>maven-snapshots</id>
                    <url>https://sonatype-nexus.backhole.ovh/repository/maven-snapshots</url>
                    <releases>
                        <enabled>false</enabled>
                    </releases>
                    <snapshots>
                        <enabled>true</enabled>
                    </snapshots>
                </repository>


            </repositories>

            <pluginRepositories>
                <!-- Référence à votre dépôt maven-central pour les plugins dans Nexus -->
                <pluginRepository>
                    <id>maven-central</id>
                    <url>https://sonatype-nexus.backhole.ovh/repository/maven-central/</url>
                    <releases>
                        <enabled>true</enabled>
                    </releases>
                    <snapshots>
                        <enabled>true</enabled>
                    </snapshots>
                </pluginRepository>

                <!-- Ajoutez des références à d'autres dépôts dans Nexus si nécessaire -->

            </pluginRepositories>
        </profile>
    </profiles>

</settings>
